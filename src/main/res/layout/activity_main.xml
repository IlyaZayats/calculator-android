<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <Button
        android:id="@+id/button0"
        android:layout_width="190dp"
        android:layout_height="65dp"
        android:layout_marginStart="12dp"
        android:layout_marginTop="5dp"
        android:text="0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/button1" />

    <Button
        android:id="@+id/buttonQuote"
        android:layout_width="95dp"
        android:layout_height="65dp"
        android:layout_marginTop="5dp"
        android:text="."
        app:layout_constraintEnd_toStartOf="@+id/buttonEquals"
        app:layout_constraintStart_toEndOf="@+id/button0"
        app:layout_constraintTop_toBottomOf="@+id/buttonPlus" />

    <Button
        android:id="@+id/buttonEquals"
        android:layout_width="95dp"
        android:layout_height="65dp"
        android:layout_marginTop="5dp"
        android:text="="
        app:layout_constraintStart_toEndOf="@+id/buttonQuote"
        app:layout_constraintTop_toBottomOf="@+id/buttonPlus" />

    <Button
        android:id="@+id/button3"
        android:layout_width="95dp"
        android:layout_height="65dp"
        android:layout_marginTop="5dp"
        android:text="3"
        app:layout_constraintEnd_toStartOf="@id/buttonPlus"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toEndOf="@id/button2"
        app:layout_constraintTop_toBottomOf="@+id/button6" />

    <Button
        android:id="@+id/button1"
        android:layout_width="95dp"
        android:layout_height="65dp"
        android:layout_marginStart="12dp"
        android:layout_marginTop="5dp"
        android:text="1"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/button4" />

    <Button
        android:id="@+id/button2"
        android:layout_width="95dp"
        android:layout_height="65dp"
        android:layout_marginTop="5dp"
        android:text="2"
        app:layout_constraintEnd_toStartOf="@id/button3"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toEndOf="@id/button1"
        app:layout_constraintTop_toBottomOf="@+id/button5" />

    <Button
        android:id="@+id/buttonPlus"
        android:layout_width="95dp"
        android:layout_height="65dp"
        android:layout_marginTop="5dp"
        android:text="+"
        app:layout_constraintStart_toEndOf="@id/button3"
        app:layout_constraintTop_toBottomOf="@+id/buttonMinus" />

    <Button
        android:id="@+id/button6"
        android:layout_width="95dp"
        android:layout_height="65dp"
        android:layout_marginTop="5dp"
        android:text="6"
        app:layout_constraintEnd_toStartOf="@id/buttonMinus"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toEndOf="@id/button5"
        app:layout_constraintTop_toBottomOf="@+id/button9" />

    <Button
        android:id="@+id/button4"
        android:layout_width="95dp"
        android:layout_height="65dp"
        android:layout_marginStart="12dp"
        android:layout_marginTop="5dp"
        android:text="4"
        app:layout_constraintEnd_toStartOf="@+id/button5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/button7" />

    <Button
        android:id="@+id/button5"
        android:layout_width="95dp"
        android:layout_height="65dp"
        android:layout_marginTop="5dp"
        android:text="5"
        app:layout_constraintEnd_toStartOf="@id/button6"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toEndOf="@id/button4"
        app:layout_constraintTop_toBottomOf="@+id/button8" />

    <Button
        android:id="@+id/buttonMinus"
        android:layout_width="95dp"
        android:layout_height="65dp"
        android:layout_marginTop="5dp"
        android:text="-"
        app:layout_constraintStart_toEndOf="@+id/button6"
        app:layout_constraintTop_toBottomOf="@+id/buttonMultiply" />

    <Button
        android:id="@+id/button8"
        android:layout_width="95dp"
        android:layout_height="65dp"
        android:layout_marginTop="5dp"
        android:text="8"
        app:layout_constraintEnd_toStartOf="@+id/button9"
        app:layout_constraintStart_toEndOf="@+id/button7"
        app:layout_constraintTop_toBottomOf="@+id/buttonDelete" />

    <Button
        android:id="@+id/button9"
        android:layout_width="95dp"
        android:layout_height="65dp"
        android:layout_marginTop="5dp"
        android:text="9"
        app:layout_constraintEnd_toStartOf="@id/buttonMultiply"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toEndOf="@id/button8"
        app:layout_constraintTop_toBottomOf="@+id/buttonDelete" />

    <Button
        android:id="@+id/buttonMultiply"
        android:layout_width="95dp"
        android:layout_height="65dp"
        android:layout_marginTop="5dp"
        android:text="*"
        app:layout_constraintStart_toEndOf="@id/button9"
        app:layout_constraintTop_toBottomOf="@+id/buttonDivison" />

    <Button
        android:id="@+id/button7"
        android:layout_width="95dp"
        android:layout_height="65dp"
        android:layout_marginStart="12dp"
        android:layout_marginTop="5dp"
        android:text="7"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/buttonDelete" />

    <Button
        android:id="@+id/buttonDivison"
        android:layout_width="95dp"
        android:layout_height="65dp"
        android:layout_marginTop="5dp"
        android:text="/"
        app:layout_constraintStart_toEndOf="@+id/buttonDelete"
        app:layout_constraintTop_toBottomOf="@+id/resultTextView" />

    <EditText
        android:id="@+id/editText2"
        android:layout_width="137dp"
        android:layout_height="60dp"
        android:layout_marginTop="80dp"
        android:layout_marginEnd="12dp"
        android:ems="10"
        android:inputType="text"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:ignore="SpeakableTextPresentCheck" />

    <EditText
        android:id="@+id/editText1"
        android:layout_width="137dp"
        android:layout_height="60dp"
        android:layout_marginStart="12dp"
        android:layout_marginTop="80dp"
        android:ems="10"
        android:inputType="text"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:ignore="SpeakableTextPresentCheck" />

    <TextView
        android:id="@+id/operationTextView"
        android:layout_width="61dp"
        android:layout_height="60dp"
        android:layout_marginStart="24dp"
        android:layout_marginEnd="24dp"
        android:gravity="center"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/editText2"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toEndOf="@+id/editText1"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.119" />

    <TextView
        android:id="@+id/resultTextView"
        android:layout_width="380dp"
        android:layout_height="52dp"
        android:layout_marginStart="12dp"
        android:layout_marginTop="5dp"
        android:gravity="center"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/operationTextView" />

    <Button
        android:id="@+id/buttonDelete"
        android:layout_width="285dp"
        android:layout_height="65dp"
        android:layout_marginStart="12dp"
        android:layout_marginTop="5dp"
        android:text="DELETE"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/resultTextView" />
</androidx.constraintlayout.widget.ConstraintLayout>